
<odoo>
    <data>
        <record id="warning_2rent_wizard_form" model="ir.ui.view">
            <field name="name">library.warning.2rent.wizard.form</field>
            <field name="model">library.warning.2rent.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="message"/>
                        </group>
                    </sheet>
                    <footer>
                        <button string="OK" class="btn-primary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
